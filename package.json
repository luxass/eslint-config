{
  "name": "@luxass/eslint-config",
  "version": "4.4.2",
  "description": "ESLint config for @luxass",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "linting",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "pnpm eslint-config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && pnpm eslint-config-inspector build",
    "test": "vitest --run",
    "typegen": "pnpx tsx ./scripts/typegen.ts",
    "test:watch": "vitest",
    "lint": "eslint . --report-unused-disable-directives",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.8",
    "@next/eslint-plugin-next": "^14.0.0",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^0.16.3",
    "eslint": ">=8.40.0",
    "eslint-plugin-astro": "^0.31.4",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-solid": "^0.1.0",
    "eslint-plugin-svelte": ">=2.37.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier-plugin-astro": "^0.13.0",
    "svelte-eslint-parser": "^0.33.1"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@stylistic/eslint-plugin": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-flat-config-utils": "^0.2.4",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-n": "^17.4.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-toml": "^0.11.0",
    "eslint-plugin-unicorn": "^52.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.25.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.2",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-plugin-prettier": "^5.0.1-1",
    "@eslint-react/eslint-plugin": "^1.5.10",
    "@eslint/config-inspector": "^0.4.8",
    "@next/eslint-plugin-next": "^14.2.3",
    "@stylistic/eslint-plugin-migrate": "^1.8.0",
    "@types/eslint": "^8.56.10",
    "@types/estree": "^1.0.5",
    "@types/node": "^20.12.7",
    "@typescript-eslint/rule-tester": "^7.8.0",
    "@unocss/eslint-plugin": "^0.59.4",
    "astro-eslint-parser": "^1.0.2",
    "bundle-require": "^4.1.0",
    "eslint": "^9.2.0",
    "eslint-plugin-astro": "^1.1.2",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-svelte": "2.38.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-typegen": "^0.2.4",
    "lint-staged": "^15.2.2",
    "prettier-plugin-astro": "^0.13.0",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^4.2.15",
    "svelte-eslint-parser": "^0.35.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
