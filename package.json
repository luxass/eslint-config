{
  "name": "@luxass/eslint-config",
  "type": "module",
  "version": "6.0.1",
  "packageManager": "pnpm@10.18.3",
  "description": "ESLint config for @luxass",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "linting",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "pnpm typegen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "pnpm eslint-config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && pnpm eslint-config-inspector build",
    "test": "vitest --run",
    "typegen": "jiti ./scripts/typegen.ts",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:prod",
    "eslint-flat-config-utils": "catalog:prod"
  },
  "devDependencies": {
    "@eslint/config-inspector": "catalog:dev",
    "@types/node": "catalog:dev",
    "eslint": "catalog:peer",
    "eslint-typegen": "catalog:dev",
    "jiti": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vitest": "catalog:dev"
  }
}
