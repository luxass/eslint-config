{
  "name": "@luxass/eslint-config",
  "version": "4.15.0",
  "description": "ESLint config for @luxass",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.5.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "linting",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "pnpm eslint-config-inspector --config eslint.config.mjs",
    "build:inspector": "pnpm build && pnpm eslint-config-inspector build",
    "test": "vitest --run",
    "typegen": "tsx ./scripts/typegen.ts",
    "test:watch": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.19.0",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": "^9.10.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "prettier-plugin-astro": "^0.14.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.9.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/markdown": "^6.2.1",
    "@stylistic/eslint-plugin": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/eslint-plugin": "^1.1.25",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-flat-config-utils": "^2.0.0",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.0.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-jsonc": "^2.18.2",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-perfectionist": "^4.6.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^1.16.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^15.14.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.0.0",
    "parse-gitignore": "^2.0.0",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.23.2",
    "@eslint/config-inspector": "^1.0.1",
    "@stylistic/eslint-plugin-migrate": "^4.0.0",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.10.5",
    "@typescript-eslint/rule-tester": "^8.20.0",
    "@unocss/eslint-plugin": "^66.0.0",
    "astro-eslint-parser": "^1.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-typegen": "^2.0.0",
    "jiti": "^2.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.15",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unocss": "^0.65.0",
    "vitest": "^3.0.7",
    "vue": "^3.5.13"
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.1",
    "@typescript-eslint/utils": "^8.19.1",
    "eslint": "^9.21.0",
    "tsx": "^4.19.3"
  }
}
