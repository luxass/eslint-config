{
  "name": "@luxass/eslint-config",
  "version": "4.1.0",
  "description": "ESLint config for @luxass",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@8.14.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "linting",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./globs": {
      "import": {
        "types": "./dist/globs.d.ts",
        "default": "./dist/globs.mjs"
      },
      "require": {
        "types": "./dist/globs.d.cts",
        "default": "./dist/globs.cjs"
      }
    },
    "./configs": {
      "import": {
        "types": "./dist/configs/index.d.ts",
        "default": "./dist/configs/index.mjs"
      },
      "require": {
        "types": "./dist/configs/index.d.cts",
        "default": "./dist/configs/index.cjs"
      }
    },
    "./configs/*": {
      "import": {
        "types": "./dist/configs/*.d.ts",
        "default": "./dist/configs/*.mjs"
      },
      "require": {
        "types": "./dist/configs/*.d.cts",
        "default": "./dist/configs/*.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=14.0.0",
    "@unocss/eslint-plugin": ">=0.50.0",
    "eslint": ">=8.40.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react": ">=7.33.2",
    "eslint-plugin-react-hooks": ">=4.6.0",
    "eslint-plugin-react-refresh": ">=0.4",
    "eslint-plugin-solid": ">=0.13.0",
    "eslint-plugin-tailwindcss": ">=3.13.0",
    "eslint-plugin-vuejs-accessibility": ">=2.2.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@antfu/install-pkg": "^0.3.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@eslint-types/jsdoc": "^47.0.2",
    "@eslint-types/typescript-eslint": "^6.17.0",
    "@eslint-types/unicorn": "^50.0.1",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint-config-flat-gitignore": "^0.1.2",
    "eslint-merge-processors": "^0.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-antfu": "^2.1.1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-jsonc": "^2.11.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-toml": "^0.8.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.20",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-yml": "^1.11.0",
    "eslint-processor-vue-blocks": "^0.1.1",
    "globals": "^13.24.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "^0.9.3",
    "vue-eslint-parser": "^9.4.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-plugin-prettier": "^5.0.1-1",
    "@next/eslint-plugin-next": "^14.0.4",
    "@stylistic/eslint-plugin-migrate": "^1.5.3",
    "@types/eslint": "^8.56.1",
    "@types/estree": "^1.0.5",
    "@types/node": "^18.17.19",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/rule-tester": "^6.18.1",
    "@typescript-eslint/utils": "^6.18.1",
    "@unocss/eslint-plugin": "^0.58.3",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-solid": "^0.13.1",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
